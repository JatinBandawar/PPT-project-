;----------------------------------------------------------------------------
; Install Sections
;----------------------------------------------------------------------------

[DestinationDirs]
CopyFiles_Drivers	= 12			; Driver directory

[CopyFiles_Drivers]
ETDHCF.sys,,,0x00000001

;----------------------------------------------------------------------------
[ETDHCF_Inst]
CopyFiles=CopyFiles_Drivers

AddReg=ETDHCF_Addreg

[ETDHCF_Inst.Services]
Include = input.inf
Needs = HID_Raw_Inst.NT.Services
AddService = ETDHCF,,ETDHCF_Service_Inst

[ETDHCF_AddReg.HW]
HKR,,"UpperFilters",0x00010000,"ETDHCF"

[ETDHCF_Inst.HW]
AddReg = ETDHCF_AddReg.HW

;----------------------------------------------------------------------------

[ETDHCF_Addreg]
HKLM,%ServiceRoot%\Elantech\ETDHCF,ETDHidI2C_Report_Type,%REG_DWORD%,1		; 0 = Regular, 1 = New Design


;----------------------------------------------------------------------------
; Services Sections
;----------------------------------------------------------------------------

[ETDHCF_Service_Inst]
DisplayName	= %ETDHCF.SVCDESC%
ServiceType   	= 1			; SERVICE_KERNEL_DRIVER
StartType     	= 3			; SERVICE_DEMAND_START
ErrorControl  	= 1			; SERVICE_ERROR_NORMAL
ServiceBinary 	= %12%\ETDHCF.sys 

